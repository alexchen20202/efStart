@model efStart3.Models.CourseAssignment

@{
    ViewData["Title"] = "Edit";
}

<h4>CourseAssignment Edit</h4><br>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input asp-for="InstructorID" />
